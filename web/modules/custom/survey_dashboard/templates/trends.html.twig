<div id="google-charts"></div>
<table id="selected-table" class="escoach-dashboard-table">
  <thead>
  <tr>
    <th>{{ title }}</th>
    <th colspan="3">Hrs/day</th>
    <th colspan="3">Hrs/week</th>
  </tr>
  <tr>
    <th class="escoach-desc escoach-trends">&nbsp;</th>
    <th class="escoach-all-day escoach-trends">All</th>
    <th class="escoach-team-day escoach-trends">My Team</th>
    <th class="escoach-me-day escoach-trends">Me</th>
    <th class="escoach-all-week escoach-trends">All</th>
    <th class="escoach-team-week escoach-trends">My Team</th>
    <th class="escoach-me-week escoach-trends">Me</th>
  </tr>
  </thead>
  <tbody>
  {% for alias, aliasData in data.aliasMap %}
    <tr>
      <td>{{ aliasData.title }}</td>
      <th>{{ data.results.all[alias].Selected.day }}</th>
      <th>{{ data.results.provider[alias].Selected.day }}</th>
      <th>{{ data.results.me[alias].Selected.day }}</th>
      <th>{{ data.results.all[alias].Selected.week }}</th>
      <th>{{ data.results.provider[alias].Selected.week }}</th>
      <th>{{ data.results.me[alias].Selected.week }}</th>
    </tr>
  {% endfor %}
  <tr>
    <td>Total Respondents</td>
    <th>{{ data.results.all.total }}</th>
    <th>{{ data.results.me.total }}</th>
    <th>{{ data.results.provider.total }}</th>
    <th>{{ data.results.all.total }}</th>
    <th>{{ data.results.me.total }}</th>
    <th>{{ data.results.provider.total }}</th>
  </tr>
  </tbody>
</table>

<div>SQL: {{ data.results.debug.query }}</div>
<div>ARGS: {{ data.results.debug.args }}</div>

