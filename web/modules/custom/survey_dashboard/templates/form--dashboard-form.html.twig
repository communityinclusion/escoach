<div class="container">
  <div class="row">
    <div class="col">{{ form.who }}</div><div class="col">{{ form.where }}</div><div class="col">{{ form.timeframe }}</div>
  </div>
  <div class="row">
    <div class="col">{{ form.what }}</div>
    <div class="col">
      <div class="row">
        {{ form.results }}
      </div>
      <div class="row">
        {{ form.submit }}
      </div>
    </div>

  </div>
  {{ form|without('who', 'what', 'where', 'results', 'timeframe', 'submit') }}
</div>

